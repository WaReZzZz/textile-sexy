            <a href="{{path('categorie', {page: 'bodys'})}}"><h1>Bodys</h1><img src="/img/Home/Carousel/image04.jpg" alt="Image 4"> </a>
            <a href="{{path('categorie', {page: 'nuisettes'})}}"><h1>Nuisettes</h1><img src="/img/Home/Carousel/image06.jpg" alt="Image 5"> </a>
