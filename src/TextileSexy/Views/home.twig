{% extends "layout.twig"%}

{% block title %}{% endblock %}

{% block css %}
    <style>
        .thumbnail {
            padding: 0px;
            border: 0px;
            margin-top: 10px;
        }
        .owl-carousel-list div h1{
            position:absolute;
            color:#000000;
            display:block;
            z-index: 2;
        }
        h1 {
            padding: 10px 10px 10px 10px;
        }
    </style>
{% endblock %}
{% block carousel %}
