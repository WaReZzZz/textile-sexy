    <img src="{{item.Variations.Item.LargeImage.URL}}" data-small-image="{{item.Variations.Item.SmallImage.URL}}" alt="Image produit">
{% elseif item.Variations.Item.ImageSets.ImageSet[0].LargeImage is defined %}
    <img src="{{item.Variations.Item.ImageSets.ImageSet[0].LargeImage.URL}}" data-small-image="{{item.Variations.Item.ImageSets.ImageSet[0].SmallImage.URL}}" alt="Image produit">
{% elseif item.Variations.Item[0].ImageSets.ImageSet.LargeImage is defined %}
    <img src="{{item.Variations.Item[0].ImageSets.ImageSet.LargeImage.URL}}" data-small-image="{{item.Variations.Item[0].ImageSets.ImageSet.SmallImage.URL}}" alt="Image produit">
