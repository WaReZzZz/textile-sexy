{% elseif item.ImageSets is defined and item.ImageSets.ImageSet.LargeImage is defined  %}
    <img src="{{item.ImageSets.ImageSet.LargeImage.URL}}" data-small-image="{{item.ImageSets.ImageSet.SmallImage.URL}}" alt="Image produit">
{% elseif item.ImageSets is defined and item.ImageSets.ImageSet[0].LargeImage %}
    <img src="{{item.ImageSets.ImageSet[0].LargeImage.URL}}" data-small-image="{{item.ImageSets.ImageSet[0].SmallImage.URL}}" alt="Image produit">
{% elseif item.Variations.Item[0].LargeImage is defined %}
    <img src="{{item.Variations.Item[0].LargeImage.URL}}" data-small-image="{{item.Variations.Item[0].SmallImage.URL}}" alt="Image produit">
